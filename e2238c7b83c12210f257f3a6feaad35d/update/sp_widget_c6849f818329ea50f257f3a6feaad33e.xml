        <script><![CDATA[(function() {
	data.assets = [];

	var gr = new GlideRecordSecure('x_1734342_pletra_0_centralized_asset_table');
	gr.query();
	while (gr.next()) {
		data.assets.push({
			"Location": gr.getDisplayValue('location'),
			"Assigned To": gr.getDisplayValue('assigned_to'),
			"Department": gr.getDisplayValue('department'),
			"Status": gr.getDisplayValue('status'),
			"Manufacturer": gr.getDisplayValue('manufacturer'),
			"Model Number": gr.getDisplayValue('model_number'),
			"Product Number": gr.getDisplayValue('product_number'),
			"Serial Number": gr.getDisplayValue('serial_number')
		});
	}
})();
]]></script>
