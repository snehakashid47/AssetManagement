    <sp_widget action="DELETE">
        <category>custom</category>
        <client_script><![CDATA[api.controller = function($timeout) {
  var c = this;

  $timeout(function() {
    var links = document.querySelectorAll('.navbar-nav li a');
    links.forEach(function(link) {
      var text = link.textContent.trim();
      if (text === "More" || text === "Get support" || text === "Tours") {
        link.style.display = 'none';
      }
    });
  }, 1000); // wait 1 second for navbar to render
};
]]></client_script>
        <controller_as>c</controller_as>
        <css>.navbar-nav li a[href*="More"],&#13;
.navbar-nav li a[href*="Get support"],&#13;
.navbar-nav li a[href*="Tours"] {&#13;
  display: none !important;&#13;
}&#13;
</css>
